{
  "name": "fastapi-app",
  "version": "0.1.0",
  "description": "FastAPI application with Svelte frontend",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"sleep 1 && npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "python cli.py serve",
    "format": "ruff format api/ tests/"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
